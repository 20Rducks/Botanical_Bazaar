<%= simple_form_for [@plant, @booking], data: { insert_in_list_target: "form", action: "submit->insert-in-list#send"} do |f| %>
  <%= f.input :start_date, as: :string, input_html: { data: { controller: "datepicker", action: "input->insert-in-list#updatePrice" }}%>
  <%= f.input :end_date, as: :string, input_html: { data: { controller: "datepicker", action: "input->insert-in-list#updatePrice", :style => 'width: 400px', class: 'd-inline' }}%>
  <%= f.submit "Book Now", class: "btn btn-flat btn btn-dark text-white mb-2" %>
<% end %>
